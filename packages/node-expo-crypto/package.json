    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "expo": "~40.0.0",
    "expo-crypto": "^9.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^40.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
